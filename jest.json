{
  "automock": true,
  "clearMocks": true,
  "unmockedModulePathPatterns": [
    "<rootDir>/node_modules/",
    "<rootDir>/src/constants\\.ts",
    "<rootDir>/src/element\\.ts$",
    "<rootDir>/src/elements/declaration\\.ts$",
    "<rootDir>/src/elements/import-export\\.ts$",
    "<rootDir>/src/elements/member\\.ts$",
    "<rootDir>/src/elements/type\\.ts$",
    "<rootDir>/src/stack\\.ts",
    "<rootDir>/src/utils/"
  ],
  "testEnvironment": "node",
  "moduleFileExtensions": ["ts", "js"],
  "mapCoverage": true,
  "coverageReporters": ["lcov", "text-summary"],
  "transform": { "\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js" },
  "testMatch": ["**/{tests,__tests__}/**/*.ts"],
  "collectCoverageFrom": ["src/**/*.ts"],
  "coveragePathIgnorePatterns": ["index.ts"],
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  }
}
